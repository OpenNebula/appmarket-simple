{
  "name": "marketplace-frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:community": "APP_TYPE=community vite --mode development",
    "dev:opennebula": "APP_TYPE=opennebula vite --mode development",
    "build:community": "APP_TYPE=community vite build --mode production",
    "build:opennebula": "APP_TYPE=opennebula vite build --mode production",
    "build:all": "npm-run-all build:community build:opennebula",
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix",
    "preview": "vite preview",
    "format": "prettier --write \"src/**/*.{tsx,ts,jsx,js}\""
  },
  "dependencies": {
    "@emotion/css": "^11.13.5",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@fontsource/inter": "^5.2.6",
    "@mui/icons-material": "^6.4.3",
    "@mui/material": "^6.4.6",
    "@mui/x-date-pickers": "^7.27.1",
    "@mui/x-date-pickers-pro": "^7.27.1",
    "@types/react-router-dom": "^5.3.3",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "fs": "^0.0.1-security",
    "iconoir-react": "^7.11.0",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "react-router": "^7.1.5",
    "react-router-dom": "^7.1.5",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@types/node": "^22.13.1",
    "@types/react": "^19.0.8",
    "@types/react-dom": "^19.0.3",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^8.57.1",
    "eslint-config-opennebula": "^2.0.4",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-webpack": "^0.13.10",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^15.14.0",
    "prettier": "^3.5.3",
    "sass-embedded": "^1.85.0",
    "typescript": "~5.7.2",
    "typescript-eslint": "^8.22.0",
    "vite": "^6.1.0"
  }
}
